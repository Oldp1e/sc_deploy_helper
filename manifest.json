{
  "manifest_version": 3,
  "name": "Desmarcar Checkboxes Publicador",
  "description": "Esta extensão desmarca automaticamente duas checkboxes no publicador ao detectar se existem na tela do usuário.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://gt-scriptcase.gtplan.net/*"],
      "js": ["jquery.min.js","content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
